{
    "name": "lasagnaphp/lasagna",
    "description": "A PHP framework for domain-driven design, event sourcing, and SOLID architecture.",
    "type": "project",
    "require": {
        "crell/tukio": "^2.0",
        "psr/container": "^2.0",
        "psr/event-dispatcher": "^1.0",
        "psr/http-client": "^1.0",
        "ramsey/uuid": "^4.9",
        "league/construct-finder": "^1.5",
        "nyholm/psr7": "^1.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.3",
        "squizlabs/php_codesniffer": "^3.13",
        "symplify/monorepo-builder": "^12.2",
        "slevomat/coding-standard": "^8.22"
    },
    "license": "Apache-2.0",
    "autoload": {
        "psr-4": {
            "Lasagna\\Lasagna\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Evan Hildreth",
            "email": "me@eph.me"
        }
    ],
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}